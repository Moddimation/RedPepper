      <memberdef kind="define" id="seadTaskMgr_8h_1a8fd5f6b6d8098e37c8fb882d3cb88548" prot="public" static="no">
        <name>SEAD_TASK_SINGLETON_IMPL</name>
        <param><defname>CLASS</defname></param>
        <initializer>    void CLASS::setInstance_(<ref refid="classsead_1_1TaskBase" kindref="compound">sead::TaskBase</ref>* task) {                                               \
        if (!sInstance) {                                                                          \
            sInstance = static_cast&lt;CLASS*&gt;(task);                                                 \
            sInstance-&gt;mSingletonDisposer.mActive = true;                                          \
        } else {                                                                                   \
            SEAD_ASSERT_MSG(false, &quot;Create Singleton Twice (%s) : addr %p&quot;, #CLASS, sInstance);    \
        }                                                                                          \
    }                                                                                              \
                                                                                                   \
    void CLASS::deleteInstance() {                                                                 \
        if (sInstance) {                                                                           \
            sInstance-&gt;mTaskMgr-&gt;destroyTaskSync(sInstance);                                       \
            sInstance = nullptr;                                                                   \
        }                                                                                          \
    }                                                                                              \
                                                                                                   \
    CLASS* CLASS::sInstance = nullptr;</initializer>
