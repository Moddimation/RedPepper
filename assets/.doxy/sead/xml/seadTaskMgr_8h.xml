        <name>SEAD_TASK_SINGLETON</name>
        <param><defname>CLASS</defname></param>
        <initializer>public:                                                                                            \
    class SingletonDisposer_ {                                                                     \
    public:                                                                                        \
        SingletonDisposer_() : mActive(false) {}                                                   \
        ~SingletonDisposer_() {                                                                    \
            if (mActive)                                                                           \
                CLASS::sInstance = nullptr;                                                        \
        }                                                                                          \
                                                                                                   \
        bool mActive;                                                                              \
    };                                                                                             \
                                                                                                   \
    static CLASS* instance() {                                                                     \
        return sInstance;                                                                          \
    }                                                                                              \
    static void setInstance_(<ref refid="classsead_1_1TaskBase" kindref="compound">sead::TaskBase</ref>* task);                                                \
    static void deleteInstance();                                                                  \
                                                                                                   \
protected:                                                                                         \
    static CLASS* sInstance;                                                                       \
                                                                                                   \
    friend class SingletonDisposer_;                                                               \
    SingletonDisposer_ mSingletonDisposer;</initializer>
